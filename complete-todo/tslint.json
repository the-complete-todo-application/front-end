{
    "defaultSeverity": "error",
    "extends": [ "tslint-react" ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false,
        "no-trailing-whitespace": false,
        "variable-name": {
            "options": ["allow-snake-case"]
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*js"
        ]
    }
}